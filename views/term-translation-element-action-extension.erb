<script type="text/javascript">

require(['jquery', 'YSDEntityManagementComplementHooks'],
        function($, EntityManagementComplementHooks) {

   TermTranslatePlugin = function() {

     this.onEdit = function(entity) {
     
       $('#term_translate a').attr('href', '/admin/cms/translate/term/' + entity.id + '?destination=/admin/cms/terms/' + entity.taxonomy_id + '/' + entity.id); //TODO Taxonomy id 
     	
     }

   }
             	
   var termTranslatePlugin = new TermTranslatePlugin();
   EntityManagementComplementHooks.register(termTranslatePlugin);	
   
});          
       
</script>