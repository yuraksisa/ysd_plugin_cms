                  <% if view_field.link %>
                    <a href="<%=view_field.evaluate_link(element)%>" <%unless view_field.link_class.to_s.empty?%>class="<%= view_field.link_class%>"<%end%>>
                  <% end %>
                  <% if view_field.image %>
                     <img src="<%=element.attribute_get(view_field.field)%>" <%unless view_field.image_class.to_s.empty?%>class="<%= view_field.image_class%>"<%end%>/>
                  <% else %>
                     <% if element.attribute_get(view_field.field).kind_of?(Time) %>
                       <%= element.attribute_get(view_field.field).strftime('%d.%m.%y') %>
                     <% else %>
                       <%= element.attribute_get(view_field.field) %>
                     <% end %>
                  <% end %>
                  <% if view_field.link %>
                    </a>
                  <% end %>
